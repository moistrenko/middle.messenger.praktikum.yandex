{
	"compilerOptions": {
		"outDir": "dist",
		"target": "es2016",
		"declaration": false,
		"module": "commonjs",
		"noImplicitAny": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"sourceMap": true,
		"strictNullChecks": true,
		"esModuleInterop": true,
	},
	"exclude": ["node_modules"],
	"include": ["src", "index.dt.ts"]
}
